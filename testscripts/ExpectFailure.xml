<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<testsuite name="ExpectFailure Tests">
	
	   
	<test name="Failure">
		<automationValue>High</automationValue>
			<teststeps>
				<expectFailure>
					<FailHere/>										
				</expectFailure>
			</teststeps>
    </test>
	   
	   
	<test name="NoFailure">
		<automationValue>High</automationValue>
			<teststeps>
				<verifyException>
					<string name = "exceptionType">ExpectedFailureException</string>
					<repeat iterations="6">
						<expectFailure>
							<string name="name">huh</string>
							<RandomGenerator method="string"  string="name"/>					
						</expectFailure>
					</repeat>
				</verifyException>
			</teststeps>
	</test>
	   
	<test name="NestedFailure">
        <automationValue>High</automationValue>
			<teststeps>
				<verifyException>
					<string name = "exceptionType">ExpectedFailureException</string>
						<expectFailure>
							<RandomGenerator>
								<string name="name">huh</string>
							</RandomGenerator>
							<expectFailure>
								<string name="name">huh</string>
								<RandomGenerator method="string"  string="name"/>
							</expectFailure>
						</expectFailure>
				</verifyException>
            </teststeps>
    </test>
	
	
	<test name="ExpectFailure no Body">
		<automationValue>High</automationValue>
			<teststeps>
				<verifyException>
					<string name = "exceptionType">ExpectedFailureException</string>
					<expectFailure>
					</expectFailure>
				</verifyException>
			</teststeps>
    </test>
	   
	   
</testsuite>